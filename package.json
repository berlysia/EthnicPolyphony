{
  "name": "ethnic-polyphony",
  "version": "0.0.1",
  "description": "Ethnic Polyphony is the twitter client built with Electron.",
  "main": "dist/index.js",
  "scripts": {
    "start": "electron dist/index.js",
    "clean": "rm -r dist/*",
    "build": "tsc -p .",
    "watch": "tsc -p . -w",
    "update": "tsconfig tsconfig.json -u",
    "activate": "cp resources/credentials.json.keep resources/credentials.json",
    "commit-ready": "cp resources/credentials.json resources/credentials.json.keep && cp resources/credentials.json.original resources/credentials.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "berlysia",
  "license": "MIT",
  "dependencies": {
    "bemmer": "^1.0.0",
    "debug": "^2.2.0",
    "eventemitter2": "^0.4.14",
    "map-iterator": "^1.1.0",
    "moment": "^2.11.2",
    "node-twitter-api": "^1.7.0",
    "option-t": "^0.18.3",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "reduce-iterator": "^1.0.0",
    "request": "^2.69.0",
    "twitter": "^1.2.5"
  },
  "devDependencies": {
    "electron-prebuilt": "^0.36.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/berlysia/EthnicPolyphony.git"
  },
  "bugs": {
    "url": "https://github.com/berlysia/EthnicPolyphony/issues"
  },
  "homepage": "https://github.com/berlysia/EthnicPolyphony#readme"
}
